package kononchik_403_java.zadanie_1;

public class Zadanie_1 {
    static int b = 2;
    static void power (int n)
    {
        int result =1;
        for (int i=0; i<n; i++) {
            result*=b;
        }
        System.out.println(result);
    }

public static void main (String[] args) {

    power (5);
    }
}
